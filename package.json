        "build": "cross-env NODE_ENV=production REACT_APP_DATABASE=PROD npx webpack --mode production -c config/webpack.config.js",
        "build:dev": "cross-env cross-env NODE_ENV=production REACT_APP_DATABASE=DEV npx webpack --mode production -c config/webpack.config.js",
